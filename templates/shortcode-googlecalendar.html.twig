{#
/**
 * @file
 * Default theme implementation for the invisible shortcode.
 *
 * Available variables:
 * - text 
 */
#}
{% if 'https://calendar.google.com/calendar/embed?' in text %}
	<ucb-calendar class="ucb-calendar ucb-google-calendar" data-query-string="{{ text|replace('https://calendar.google.com/calendar/embed?', '') }}">&nbsp;</ucb-calendar>
{% else %}
	<strong>Not a valid Google calendar.</strong>
{% endif %}


